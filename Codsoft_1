#include<iostream>
#include<cstdlib>
using namespace std;
int main(){
    int x,n;
    x=rand()%100+1;
    while(true){
        cout<<"Enter a number: ";
        cin>>n;
        if(n>x){
            cout<<"too high"<<endl;
        }
        else if(n<x){
            cout<<"Too low"<<endl;
        }
        else{
            cout<<"you guessed the correct number!"<<endl;
        }
    }
}
